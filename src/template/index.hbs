<html>
  <head>
    <meta charset="utf-8">
    <title>Feedgen feeds</title>
  </head>

  <body>
    <h1>Feedgen feed list</h1>
    <ul>
      {{~#each feeds}}
      <li><strong>{{this.name}}:</strong> <a href="{{this.rss_url}}">RSS</a> (entries: {{this.entry_count}}), <a href="{{this.fetch_url}}">source</a> (last updated: {{this.last_updated}})</li>
      {{~/each}}
    </ul>
  </body>
</html>
